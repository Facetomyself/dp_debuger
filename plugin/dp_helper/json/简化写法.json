[
    {
        "原写法": "@id",
        "简化写法": "#",
        "精确匹配": "#或#=",
        "模糊匹配": "#:",
        "匹配开头": "#^",
        "匹配结尾": "#$",
        "备注": "简化写法只能单独使用"
    },
    {
        "原写法": "@class",
        "简化写法": ".",
        "精确匹配": ".或.=",
        "模糊匹配": ".:",
        "匹配开头": ".^",
        "匹配结尾": ".$",
        "备注": "简化写法只能单独使用"
    },
    {
        "原写法": "tag",
        "简化写法": "t",
        "精确匹配": "t:或t=",
        "模糊匹配": "无",
        "匹配开头": "无",
        "匹配结尾": "无",
        "备注": "只能用在句首"
    },
    {
        "原写法": "@tag()",
        "简化写法": "@t()",
        "精确匹配": "@t():或@t()=",
        "模糊匹配": "无",
        "匹配开头": "无",
        "匹配结尾": "无",
        "备注": "可作为属性组合使用"
    },
    {
        "原写法": "text",
        "简化写法": "tx",
        "精确匹配": "tx=",
        "模糊匹配": "tx:或不写",
        "匹配开头": "tx^",
        "匹配结尾": "tx$",
        "备注": "无标签时使用模糊匹配文本"
    },
    {
        "原写法": "@text()",
        "简化写法": "@tx()",
        "精确匹配": "@tx()=",
        "模糊匹配": "@tx():",
        "匹配开头": "@tx()^",
        "匹配结尾": "@tx()$",
        "备注": "可作为属性组合使用"
    },
    {
        "原写法": "xpath",
        "简化写法": "x",
        "精确匹配": "x:或x=",
        "模糊匹配": "无",
        "匹配开头": "无",
        "匹配结尾": "无",
        "备注": "只能单独使用"
    },
    {
        "原写法": "css",
        "简化写法": "c",
        "精确匹配": "c:或c=",
        "模糊匹配": "无",
        "匹配开头": "无",
        "匹配结尾": "无",
        "备注": "只能单独使用"
    }
]