{
  "startup": {
    "url": "https://www.nodeseek.com/",
    "inject": true,
    "listen": false,
    "loadPlugins": true,
    "chrome": "",
    "plugins": ["AntiDebug_Breaker-main", "dp_helper"]
  },
  "debugUtils": {
    "interceptXHR": true,
    "interceptFetch": true,
    "startDOMObserver": false,
    "startPerformanceMonitorMs": 0
  },
  "cookieMonitor": {
    "ignoreUnchanged": false,
    "enableEvent": { "add": true, "update": true, "delete": true, "read": true },
    "rules": [
      "session",
      { "events": "add|update", "name": "auth_token" },
      { "events": "delete", "name": "/token_.+/" }
    ]
  }
}
